{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/todo-list",
  "version": "0.1.0",
  "title": "Todo List",
  "category": "widgets",
  "icon": "smiley",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "supports": {
    "html": true,
    "align": [
      "left",
      "center",
      "right",
      "wide",
      "full"
    ],
    "anchor": true,
    "color": {
      "background": false,
      "text": true,
      "gradients": true
    },
    "multiple": false,
    "customClassName": true,
    "defaultStylePicker": true,
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "__experimentalFontFamily": true
    }
  },
  "styles": [
    {
      "name": "default",
      "label": "Default",
      "isDefault": true
    },
    {
      "name": "fancy",
      "label": "Fancy"
    }
  ],
  "attributes": {
    "content": {
      "type": "string",
      "source": "html",
      "selector": "h2"
    }
  },
  "textdomain": "todo-list",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}